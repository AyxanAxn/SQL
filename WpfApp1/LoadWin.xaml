<Window x:Class="WpfApp1.LoadWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="LoadWin" Height="450" Width="600">
    <Grid>
        <StackPanel>
            <TextBlock HorizontalAlignment="Center">Vendor</TextBlock>
            <TextBox Name="vendor" Margin="20" FontSize="20"></TextBox>
            <TextBlock HorizontalAlignment="Center">Model</TextBlock>
            <TextBox Name="model" Margin="20" FontSize="20"></TextBox>
            <TextBlock HorizontalAlignment="Center">Year</TextBlock>
            <TextBox Name="year" Margin="20" FontSize="20"></TextBox>
            <TextBlock HorizontalAlignment="Center">Is new</TextBlock>
            <TextBox Name="isNew" Margin="20" FontSize="20"></TextBox>
            <Button  Margin="30,20" HorizontalAlignment="Center" FontSize="30" Click="Button_Click">  Save</Button>
        </StackPanel>
    </Grid>
</Window>
